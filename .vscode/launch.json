{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch AppRazor (net9.0)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/AppRazor/bin/Debug/net9.0/AppRazor.dll",
      "args": [],
      "cwd": "${workspaceFolder}/AppRazor",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/AppRazor/Views"
      }
    },
    {
      "name": "Launch AppWebApi (net9.0)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/AppWebApi/bin/Debug/net9.0/AppWebApi.dll",
      "args": [],
      "cwd": "${workspaceFolder}/AppWebApi",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
        "uriFormat": "%s/swagger"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": "Launch AppMVC (net9.0)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/AppMVC/bin/Debug/net9.0/AppMVC.dll",
      "args": [],
      "cwd": "${workspaceFolder}/AppMVC",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/AppMVC/Views"
      }
    }
  ]
}